{
	"id": "rb-unit-tests",
	"paths": ["src", "test", "deps/closure-library/third_party/closure/goog"],
	"closure-library" : "deps/closure-library/closure/goog/",
	"mode" : "ADVANCED",
	"level" : "VERBOSE",
	"pretty-print" : false,
	"output-charset": "UTF-8",
    "inputs" : [
        "test/unit/common/net/restmanager.js",
        "test/unit/common/cache/localcache.js",
        "test/unit/common/storage/localstorage.js",
        "test/unit/common/storage/rest.js"
    ],
    "output-file" : "test/allUnitTests.js",
    "experimental-compiler-options": {
        "sourceMapFormat": "V3"
    },
    "externs" : ["node_modules/nclosure/lib/externs.js", "lib/externs/mocha.js"]
}
